@prefix eurostat: <http://ontologycentral.com/2009/01/eurostat/ns#> .
@prefix ex: <http://example.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix geo-vocab: <http://geovocab.org/spatial#> .
@prefix http: <http://www.w3.org/2011/http#> .
@prefix http_m: <http://www.w3.org/2011/http-methods#> .
@prefix httpm: <http://www.w3.org/2011/http-methods#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix olap4ld: <http://example.org/olap4ld#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix qbe: <http://citydata.wu.ac.at/qb#> .
@prefix qrl: <http://www.aifb.kit.edu/project/ld-retriever/qrl#> .
@prefix ramon: <http://rdfdata.eionet.europa.eu/ramon/ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#> .
@prefix sdmx-measure: <http://purl.org/linked-data/sdmx/2009/measure#> .
@prefix sdmxdim: <http://purl.org/linked-data/sdmx/2009/dimension#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosclass: <http://ddialliance.org/ontologies/skosclass#> .
@prefix spatial: <http://geovocab.org/spatial#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ALL-value a skos:Concept ;
    rdfs:label "Special type ALL dimension value"@en .

:global-cube-ds a qb:DataSet ;
    rdfs:label "Open City Data Pipeline Global Cube"@en ;
    qb:structure :global-cube-dsd .

:global-cube-dsd a qb:DataStructureDefinition ;
    rdfs:label "Open City Data Pipeline Global Cube Data Structure Definition"@en ;
    qb:component [ qb:dimension cd:hasIndicator ],
        [ qb:dimension sdmx-dimension:refArea ],
        [ qb:dimension dcterms:date ],
        [ qb:dimension sdmx-dimension:sex ],
        [ qb:dimension eurostat:unit ],
        [ qb:dimension sdmx-dimension:age ],
        [ qb:dimension dcterms:publisher ],

	[ qb:attribute cd:estimatedRMSE ],
	[ qb:attribute cd:preferredObservation ],
	[ qb:attribute prov:wasGeneratedBy ],
	[ qb:attribute prov:generatedAtTime ],

        [ qb:measure sdmx-measure:obsValue ] .

cd:hasIndicator a qb:DimensionProperty,
        rdf:Property ;
    rdfs:range cd:Indicator .

eurostat:unit a qb:DimensionProperty,
        rdf:Property .

dcterms:date a qb:DimensionProperty,
        rdf:Property ;
    rdfs:range db:Year .

dcterms:publisher a qb:DimensionProperty,
        rdf:Property .

sdmx-dimension:age a qb:DimensionProperty,
        rdf:Property .

sdmx-dimension:refArea a qb:DimensionProperty,
        rdf:Property ;
    rdfs:range db:City .

sdmx-dimension:sex a qb:DimensionProperty,
        rdf:Property .

sdmx-measure:obsValue a qb:MeasureProperty,
        rdf:Property ;
    rdfs:range xsd:decimal .

cd:estimatedRMSE a qb:AttributeProperty,
	rdf:Property ;
	rdfs:range xsd:decimal .

cd:preferredObservation a qb:AttributeProperty,
	rdf:Property ;
	rdfs:range qb:Observation .

prov:wasGeneratedBy a qb:AttributeProperty,
        rdf:Property ;
        rdfs:range qb:Observation .

prov:generatedAtTime a qb:AttributeProperty,
        rdf:Property ;
        rdfs:range qb:Observation .
